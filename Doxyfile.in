# Doxyfile configuration for massive-cpp

PROJECT_NAME           = "massive-cpp"
PROJECT_NUMBER         = @PROJECT_VERSION@
PROJECT_BRIEF          = "Modern C++20 SDK for the Massive REST and WebSocket APIs"
OUTPUT_DIRECTORY       = docs
INPUT                  = include
RECURSIVE              = YES
FILE_PATTERNS          = *.hpp *.cpp
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = NO
EXTRACT_STATIC         = YES
GENERATE_HTML          = YES
GENERATE_LATEX         = NO
HTML_OUTPUT            = html
USE_MDFILE_AS_MAINPAGE = README.md
MARKDOWN_SUPPORT       = YES
AUTOLINK_SUPPORT       = YES
GENERATE_TREEVIEW      = YES
DISABLE_INDEX          = NO
FULL_SIDEBAR           = YES
HAVE_DOT               = NO
WARN_IF_UNDOCUMENTED   = YES
WARN_IF_DOC_ERROR      = YES
QUIET                  = NO
WARNINGS               = YES
WARN_LOGFILE           = docs/warnings.log

